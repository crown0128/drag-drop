{
  "name": "drag-drop",
  "description": "HTML5 drag & drop for humans",
  "version": "5.0.1",
  "author": "Feross Aboukhadijeh <feross@feross.org> (http://feross.org/)",
  "bugs": {
    "url": "https://github.com/feross/drag-drop/issues"
  },
  "dependencies": {
    "blob-to-buffer": "^1.0.2",
    "run-parallel": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^16.1.0",
    "ecstatic": "^4.0.0",
    "html-inline": "^1.2.0",
    "standard": "*",
    "tape": "^4.0.0",
    "tinyify": "^2.5.2"
  },
  "homepage": "https://github.com/feross/drag-drop",
  "keywords": [
    "drag",
    "drop",
    "dnd",
    "drag and drop",
    "drag drop",
    "html5",
    "drag & drop",
    "frontend",
    "browserify"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/drag-drop.git"
  },
  "scripts": {
    "build": "browserify --plugin tinyify -s DragDrop -r ./ > dragdrop.min.js",
    "demo": "html-inline example.html | curl -sT- https://htmlb.in",
    "example": "npm run build && ecstatic",
    "size": "npm run build && cat dragdrop.min.js | gzip | wc -c",
    "test": "standard && tape test/*.js"
  }
}
